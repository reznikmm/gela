Rules for identifier.identifier_token :
(.
      --  choose between Function_Call and Identifier
      ${identifier.chosen_interpretation} := Gela.Interpretations.Identifier;
      Gela.Pass_Utils.Choose_Auxiliary_Apply_Interpretation
        (Self.Compilation,
         ${identifier.Down},
         ${identifier.chosen_interpretation});
.)

Rules for composite_constraint. :
(.
      --  choose between Function_Call and Indexed_Component
      --   ${composite_constraint.Down},
      Gela.Pass_Utils.Choose_Composite_Constraint_Interpretation
        (Self.Compilation,
         Node,
         ${composite_constraint.chosen_interpretation});
.)

Rules for number_declaration. :
(.
      --  choose between Integer_Number and Real_Number
      --  ${number_declaration.env_in},
      --  ${Initialization_Expression.Up},
      Gela.Pass_Utils.Choose_Number_Declaration_Interpretation
        (Self.Compilation,
         Node,
         ${number_declaration.chosen_interpretation});
.)
