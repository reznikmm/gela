Synthesized attributes
  function_call,
  identifier,
  composite_constraint,
  number_declaration
   : Gela.Interpretations.Interpretation_Kinds : chosen_interpretation;

Rules for function_call. :
(.
      --  choose between Function_Call and Indexed_Component
      ${function_call.chosen_interpretation} :=
        Gela.Interpretations.Function_Call;
      Gela.Pass_Utils.Choose_Auxiliary_Apply_Interpretation
        (Self.Compilation,
         ${function_call.Down},
         ${function_call.chosen_interpretation});

