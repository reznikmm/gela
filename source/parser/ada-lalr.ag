      List : Node :=
        Self.Factory.Select_Then_Abort_Path_Sequence;
   begin
      Self.Factory.Append_Select_Then_Abort_Path
        (List, Item);
      Self.Factory.Append_Select_Then_Abort_Path
        (List, Then_Item);
      ${asynchronous_select.ast} := Self.Factory.Asynchronous_Select
        (${select_token.ast},
         List,
