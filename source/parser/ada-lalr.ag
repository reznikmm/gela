Rules for Compilation. :
(.
   declare
      List : Node :=
        ${compilation_unit_list.ast:Self.Factory.Compilation_Unit_Sequence};
   begin
      Self.Factory.Prepend_Compilation_Unit
        (List, ${compilation_unit.ast});
      ${Compilation.ast} := Self.Factory.Compilation
         (List, ${context_item_list.ast:(Self.Factory.Context_Item_Sequence)});
