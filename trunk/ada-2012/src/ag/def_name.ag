Synthesized attributes
  character_literal,
  enumeration_literal,
  identifier,
  operator_symbol,
  selector_name
   : Gela.Elements.Defining_Names.Defining_Name_Access : Defining_Name;

Rules for identifier.identifier_token :
(.
      Self.Compilation.Context.Interpretation_Manager.Get_Defining_Name
        (${identifier.Down},
         ${identifier.Defining_Name});
.)

Rules for character_literal.character_literal_token :
(.
        ${character_literal.Defining_Name} := null;
.)

Rules for enumeration_literal.enumeration_literal_token :
(.
        ${enumeration_literal.Defining_Name} := null;
.)

Rules for operator_symbol.operator_symbol_token :
(.
      Self.Compilation.Context.Interpretation_Manager.Get_Defining_Name
        (${operator_symbol.Down},
         ${operator_symbol.Defining_Name});
.)

